<% if (breadcrumbs) {%>
    <ol class="breadcrumb" style="text-transform:capitalize;">
        <li>
            <i class="fa fa-home"></i>&nbsp;
        </li>
    <% breadcrumb.forEach( function (message){ %>
        <% if( @last ){ %>
            <li>
                <span><%= this.label %></span>
            </li>
        <% }else{ %>
            <li>
                <a href="<%= this.url %>">
                    <span><%= this.label %></span>
                </a>
                <span>&nbsp; / &nbsp;</span>
            </li>
        <% }%>
    <% }) %>
    </ol>
    
<%}%>